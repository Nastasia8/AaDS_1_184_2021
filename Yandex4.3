def search(A, n):
    stack = [n-1]
    b = [0]*n
    b[n-1] = -1
    for i in range(n-2, -1, -1):
        if A[stack[-1]] >= A[i]:
            while (stack != []) and (A[stack[-1]] >= A[i]):
                stack.pop()
            if (stack == []):
                b[i] = -1
            else:
                b[i] = stack[-1]
            stack.append(i)
        else:
            b[i] = stack[-1]
            stack.append(i)
    return b
            

N = int(input())
A = list(map(int, input().split()))
A = search(A, N)
print(' '.join(map(str, A)))