def funct(n,m):
    a =[]
    for x in range (1,31):
        if x%2==1:
            a.append(x**n**m)
        else:
            a.append(x)
    print(a)
n=int(input("Введите n:"))
m=int(input("Введите m:"))
funct(n,m)
print([number**n**m if number%2==1 else number for number in range(1,31)])
