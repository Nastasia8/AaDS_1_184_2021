def merge_sort(M, kt):

    if len(M) > 1:
        mid = len(M)//2
        left = M[:mid]
        right = M[mid:]
        
        kt = merge_sort(left, kt)
        kt = merge_sort(right, kt)

        x,y,z = 0,0,0
        while x < len(left) and y < len(right):
            if left[x] <= right[y]:
                M[z] = left[x]
                x += 1
            else:
                M[z] = right[y]
                y = y+1
                kt += len(left)-x
            z = z+1
        while x < len(left):
            M[z] = left[x]
            x = x+1
            z = z+1
        while y < len(right):
            M[z] = right[y]
            y = y+1
            z = z+1

    return kt


kt = 0
n = int(input())
M = list(map(int, input().split()))[:n]
kt = merge_sort(M, kt)
print(kt)